# Minimal Trading Bot Configuration
# Copy this to .env and fill in your credentials

# Core Bot Configuration (REQUIRED)
TELEGRAM_TOKEN=your_telegram_bot_token_here
BYBIT_API_KEY=your_bybit_api_key_here
BYBIT_API_SECRET=your_bybit_api_secret_here
USE_TESTNET=false

# OpenAI Integration (OPTIONAL - for AI features)
# Leave empty to disable AI features
OPENAI_API_KEY=

# Social Media APIs (DISABLED - not needed for core functionality)
ENABLE_SOCIAL_SENTIMENT=false